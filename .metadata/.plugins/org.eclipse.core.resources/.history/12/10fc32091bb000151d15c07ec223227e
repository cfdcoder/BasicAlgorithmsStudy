//define the nodes in the binary search tree
class Node{
	int key;
	String data;
	
	Node leftChild;
	Node rightChild;
	
	Node(int key, String name){
		this.key=key;
		this.data=data;
	}
	
	public String toString(){
		return this.data+" has a key"+ this.key;
	}
}